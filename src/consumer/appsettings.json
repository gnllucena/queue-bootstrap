{
  "Database": {
    "Server": "localhost",
    "Port": "3306",
    "Database": "api",
    "User": "root",
    "Password": "root"
  },
  "Cache": {
    "Addresses": [
      "localhost"
    ],
    "Port": 6379,
    "Password": "admin"
  },
  "Messaging": {
    "Host": "localhost",
    "VirtualHost": "/",
    "Port": 5672,
    "User": "admin",
    "Password": "admin",
    "Durable": true,
    "TTL": 1200000,
    "Retries": 3,
    "Consuming": {
      "Queue": "anothercontext.consuming",
      "Bindingkey": "anothercontext.consuming",
      "Exchange": {
        "name": "anothercontext",
        "type": "direct"
      },
      "Deadletter": {
        "Queue": "errors.anothercontext.consuming",
        "RoutingKey": "errors.anothercontext.consuming",
        "Exchange": {
          "name": "errors",
          "type": "direct"
        }
      }
    },
    "Publishing": {
      "Queue": "yetanothercontext.consuming",
      "RoutingKey": "yetanothercontext.consuming",
      "Exchange": {
        "name": "yetanothercontext",
        "type": "direct"
      },
      "Deadletter": {
        "Queue": "errors.yetanothercontext.consuming",
        "RoutingKey": "errors.yetanothercontext.consuming",
        "Exchange": {
          "name": "errors",
          "type": "direct"
        }
      }
    },
    "Error": {
      "Queue": "errors.global",
      "RoutingKey": "errors.global",
      "Exchange": {
        "name": "errors",
        "type": "direct"
      }
    }
  }
}